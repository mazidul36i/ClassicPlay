{
    "hosting": {
        "site": "classicplay",
        "public": "tic-tac-toe/src",
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
        "rewrites": [
            {
                "source": "/",
                "destination": "/index.html"
            },
            {
                "source": "**",
                "destination": "/$&.html"
            }
        ]
    }
}
